/* pages/mine/index.wxss */
.bgtop{
  height:320rpx;
  background: url('https://pic.chuangcheng8.com/xcx/scmx_top_bg.png') no-repeat;
  background-size:100%;
}
.aniScale{
  animation: breathing 0.6s ease-in-out .2s 2 alternate;
}
@keyframes breathing {
  0%{
    transform: scale(0.93);
  }
  100%{
    transform: scale(1);
  }
}